#!/bin/sh
#
# Save build products to the Subversion area for packaging.
# Make sure that the release area has been cleaned and
# VERS, DATE files updated
#
#set -x
REPO_ROOT=~/Development/ils/src
SVN=~/Development/ils/svn/EMChemicals/Release/src
# Prepare directories
mkdir -p $SVN/project
cd $REPO_ROOT/sfc/project
cp sfc_demo_proj.proj $SVN/project
cp BrowserGlobalDemo.proj $SVN/project
SVN=~/Development/ils/svn/EMChemicals/Ignition
cp $REPO_ROOT/sfc/workspace/sfc-build/modules/*.modl $SVN
