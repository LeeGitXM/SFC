#!/bin/sh
#
# Save build products to the Subversion area for packaging.
# Make sure that the release area has been cleaned and
# VERS, DATE files updated
#
#set -x
SVN=${SVN_REPO}/EMChemicals/Release/src
# Prepare directories
mkdir -p $SVN/project
cd ${GIT_REPO}/sfc/project
cp BrowserGlobalDemo.proj $SVN/project
SVN=${SVN_REPO}/EMChemicals/Ignition
cp ${GIT_REPO}/sfc/workspace/sfc-build/modules/*.modl $SVN
